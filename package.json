    "lib": "rm -rf lib && mkdir lib && cp -a src/. lib && babel lib -d lib"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "block-loader": "^2.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "highlight.js": "^9.10.0",
    "import": "0.0.6",
    "jest": "^18.1.0",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "radium": "^0.19.1",
    "react-motion": "^0.4.7"
  },
  "jest": {
    "rootDir": "src",
    "testRegex": "(/__tests__/.*)\\.js$"
  }
}
